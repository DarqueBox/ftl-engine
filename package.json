{
  "name": "ftl-engine",
  "version": "0.1.1-beta",
  "description": "A scalable, fault-tolerant distributed task runner for complex workflows",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run build",
    "test": "nyc --all npm run test_real && npm run lint",
    "test_real": "mocha --require source-map-support/register build/test/*Test.js build/test/**/*Test.js build/test/**/**/*Test.js",
    "upload_cov": "nyc report --reporter=json && codecov -f coverage/*.json",
    "testSingle": "./testSingle.sh",
    "lint": "tslint src/*.ts src/**/*.ts",
    "clean": "rm -rf build",
    "build-full": "npm run clean && npm run typings && tsc",
    "build": "tsc && ./copyNonTs.sh",
    "typings": "npm run typings-clean && typings install",
    "typings-clean": "rm -rf typings",
    "prepublish": "npm run build-full",
    "dev": "ftl-engine -c dev-config.js server"
  },
  "nyc": {
    "include": [
      "build/src/*.js",
      "build/src/**/*.js",
      "build/src/**/**/*.js"
    ]
  },
  "bin": {
    "ftl-engine": "./bin/ftl-engine"
  },
  "keywords": [
    "workflow",
    "etl",
    "graph-execution",
    "ftl",
    "engine"
  ],
  "author": "addisonj",
  "license": "MIT",
  "devDependencies": {
    "@types/async": "2.0.32",
    "@types/bunyan": "0.0.31",
    "@types/chai": "^3.4.34",
    "@types/es6-promise": "0.0.32",
    "@types/lodash": "^4.14.37",
    "@types/mocha": "^2.2.32",
    "@types/shortid": "0.0.28",
    "@types/sinon": "^1.16.31",
    "@types/statsd-client": "^0.1.30",
    "@types/yargs": "0.0.34",
    "bunyan-prettystream": "^0.1.3",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "mocha": "^2.4.5",
    "nyc": "^10.0.0",
    "nodemon": "^1.11.0",
    "sinon": "^1.17.3",
    "source-map-support": "^0.4.6",
    "tslint": "^3.7.4",
    "typescript": "^2.0.0",
    "typings": "^1.3.1"
  },
  "dependencies": {
    "async": "^2.0.32",
    "aws-sdk": "^2.4.7",
    "browserify-middleware": "^7.0.0",
    "bunyan": "^1.8.1",
    "bunyan-prettystream": "^0.1.3",
    "express": "^4.14.0",
    "lodash": "^4.10.0",
    "material-ui": "^0.16.0",
    "moment": "^2.15.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "shortid": "^2.2.6",
    "simple-swf": "^0.1.1-beta",
    "source-map-support": "^0.4.0",
    "statsd-client": "^0.2.2",
    "superagent": "^2.1.0",
    "yargs": "4.8.1"
  }
}
