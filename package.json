{
  "name": "ftl-engine",
  "version": "0.1.1-beta",
  "description": "A scalable, fault-tolerant distributed task runner for complex workflows",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run build",
    "test": "nyc --all npm run test_real && npm run lint",
    "test_real": "mocha --require source-map-support/register build/test/*Test.js build/test/**/*Test.js build/test/**/**/*Test.js",
    "upload_cov": "nyc report --reporter=json && codecov -f coverage/*.json",
    "testSingle": "./testSingle.sh",
    "lint": "tslint src/*.ts src/**/*.ts",
    "clean": "rm -rf build build-app",
    "build-full": "npm run clean && npm run typings && npm run build",
    "build": "tsc && webpack --config webpack.vendor-dll.js && webpack",
    "build:quick": "tsc && webpack --config webpack.vendor-dll.js",
    "typings": "npm run typings-clean && typings install",
    "typings-clean": "rm -rf typings",
    "prepublish": "NODE_ENV=production npm run build-full",
    "dev:server": "ftl-engine -c dev-config.js server",
    "dev": "./bin/start-dev"
  },
  "nyc": {
    "include": [
      "build/src/*.js",
      "build/src/**/*.js",
      "build/src/**/**/*.js"
    ]
  },
  "bin": {
    "ftl-engine": "./bin/ftl-engine"
  },
  "keywords": [
    "workflow",
    "etl",
    "graph-execution",
    "ftl",
    "engine"
  ],
  "author": "addisonj",
  "license": "MIT",
  "devDependencies": {
    "@types/async": "2.0.32",
    "@types/bunyan": "0.0.31",
    "@types/chai": "^3.4.34",
    "@types/es6-promise": "0.0.32",
    "@types/express": "^4.0.33",
    "@types/express-serve-static-core": "^4.0.37",
    "@types/history": "2.0.46",
    "@types/lodash": "4.14.37",
    "@types/material-ui": "^0.15.37",
    "@types/mime": "0.0.29",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.45",
    "@types/react": "^0.14.41",
    "@types/react-dom": "^0.14.18",
    "@types/react-redux": "^4.4.32",
    "@types/react-router": "2.0.38",
    "@types/react-router-redux": "4.0.34",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/redux": "3.6.31",
    "@types/redux-actions": "0.8.30",
    "@types/redux-logger": "2.6.32",
    "@types/redux-thunk": "2.1.31",
    "@types/serve-static": "1.7.31",
    "@types/shortid": "0.0.28",
    "@types/sinon": "^1.16.31",
    "@types/statsd-client": "^0.1.30",
    "@types/superagent": "^2.0.35",
    "@types/yargs": "0.0.34",
    "awesome-typescript-loader": "2.2.4",
    "bunyan-prettystream": "^0.1.3",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "copy-webpack-plugin": "^3.0.1",
    "d3-force": "^1.0.3",
    "material-ui": "^0.16.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.11.0",
    "nyc": "^10.0.0",
    "react-dom": "15.2.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "4.4.5",
    "react-router": "2.6.0",
    "react-router-redux": "4.0.5",
    "react-tap-event-plugin": "1.0.0",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.1",
    "redux-form": "^6.1.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "sinon": "^1.17.3",
    "source-map-support": "^0.4.6",
    "tslint": "^3.7.4",
    "typescript": "2.0.10",
    "typings": "^1.3.1",
    "webpack-dev-server": "^2.1.0-beta.9"
  },
  "dependencies": {
    "async": "^2.0.32",
    "aws-sdk": "^2.4.7",
    "bunyan": "^1.8.1",
    "bunyan-prettystream": "^0.1.3",
    "express": "^4.14.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.10.0",
    "moment": "^2.15.1",
    "react": "15.2.1",
    "shortid": "^2.2.6",
    "simple-swf": "^0.2.0-beta",
    "source-map-support": "^0.4.0",
    "statsd-client": "^0.2.2",
    "superagent": "^2.1.0",
    "webpack": "^2.1.0-beta.25",
    "yargs": "4.8.1"
  }
}
